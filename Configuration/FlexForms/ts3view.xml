<T3DataStructure>
    <sheets>
        <sDef>
            <ROOT>
                <TCEforms>
                    <sheetTitle>WOW Stats</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                  <settings.server.ip>
                    <TCEforms>
                      <label>TS IP</label>
                      <config>
                        <type>text</type>
                        <rows>1</rows>
                        <eval>required,trim</eval>
                      </config>
                    </TCEforms>
                  </settings.server.ip>
                  <settings.server.port>
                    <TCEforms>
                      <label>TS Port</label>
                      <config>
                        <type>text</type>
                        <rows>1</rows>
                        <eval>required,int</eval>                        
                      </config>
                    </TCEforms>
                  </settings.server.port>    
                  <settings.server.queryadmin>
                    <TCEforms>
                      <label>TS Query Admin</label>
                      <config>
                        <type>text</type>
                        <rows>1</rows>
                        <eval>required,trim</eval>                        
                      </config>
                    </TCEforms>
                  </settings.server.queryadmin> 
                  <settings.server.querypw>
                    <TCEforms>
                      <label>TS Query Password</label>
                      <config>
                        <type>text</type>
                        <rows>1</rows>
                        <eval>required,trim</eval>                        
                      </config>
                    </TCEforms>
                  </settings.server.querypw>   
                  <settings.server.queryport>
                    <TCEforms>
                      <label>TS Query Port</label>
                      <config>
                        <type>text</type>
                        <rows>1</rows>
                        <eval>required,int</eval>                        
                      </config>
                    </TCEforms>
                  </settings.server.queryport>   
                  <settings.server.queryusernick>
                    <TCEforms>
                      <label>TS Query Usernick</label>
                      <config>
                        <type>text</type>
                        <rows>1</rows>
                        <eval>trim</eval>                        
                      </config>
                    </TCEforms>
                  </settings.server.queryusernick>                    
                </el>
            </ROOT>
        </sDef>
    </sheets>
</T3DataStructure>